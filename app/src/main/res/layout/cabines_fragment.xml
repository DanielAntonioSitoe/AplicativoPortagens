<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".ui.monitorar.CabinesFragment">

    <LinearLayout
        android:layout_width="0px"
        android:layout_height="0px"
        app:layout_constraintHeight_percent="0.80"
        app:layout_constraintWidth_percent="0.90"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <LinearLayout
            android:layout_width="130dp"
            android:layout_height="match_parent"
            app:layout_constraintWidth_percent="0.90"
            android:gravity="center"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_margin="12dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <Button
                android:id="@+id/cabine1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="5dp"
                android:paddingVertical="20dp"
                android:background="@drawable/custom_button"
                android:textColor="@android:color/white"
                android:text="Cabine1" />


            <Button
                android:id="@+id/cabine2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="5dp"
                android:paddingVertical="20dp"
                android:background="@drawable/custom_button"
                android:textColor="@android:color/white"
                android:text="Cabine2" />

            <Button
                android:id="@+id/cabine3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="5dp"
                android:paddingVertical="20dp"
                android:background="@drawable/custom_button"
                android:textColor="@android:color/white"
                android:text="Cabine3" />

            <Button
                android:id="@+id/cabine4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="5dp"
                android:paddingVertical="20dp"
                android:background="@drawable/custom_button"
                android:textColor="@android:color/white"
                android:text="Cabine4" />
            <Button
                android:id="@+id/cabine5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="5dp"
                android:paddingVertical="20dp"
                android:background="@drawable/custom_button"
                android:textColor="@android:color/white"
                android:text="Cabine5" />
            <Button
                android:id="@+id/cabine6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="5dp"
                android:paddingVertical="20dp"
                android:background="@drawable/custom_button"
                android:textColor="@android:color/white"
                android:text="Cabine6" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_margin="12dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <Button
                android:id="@+id/cabine7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="5dp"
                android:paddingVertical="20dp"
                android:background="@drawable/custom_button"
                android:textColor="@android:color/white"
                android:text="Cabine7" />


            <Button
                android:id="@+id/cabine8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginVertical="5dp"
                android:paddingVertical="20dp"
                android:background="@drawable/custom_button"
                android:textColor="@android:color/white"
                android:text="Cabine8" />

            <Button
                android:id="@+id/cabine9"
                android:layout_width="match_parent"
